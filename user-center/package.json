{
	"name": "user-center",
	"version": "1.0.0",
	"private": true,
	"description": "小程序",
	"templateInfo": {
		"name": "redux",
		"typescript": true,
		"css": "sass"
	},
	"scripts": {
		"build:weapp": "taro build --type weapp",
		"build:weapp:dev": "cross-env BUILD_ENV=dev taro build --type weapp",
		"build:weapp:test": "cross-env BUILD_ENV=test taro build --type weapp",
		"build:weapp:demo": "cross-env BUILD_ENV=demo taro build --type weapp",
		"build:weapp:prod": "cross-env BUILD_ENV=prod taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:h5:dev": "cross-env BUILD_ENV=dev taro build --type h5",
		"build:h5:test": "cross-env BUILD_ENV=test taro build --type h5",
		"build:h5:demo": "cross-env BUILD_ENV=demo taro build --type h5",
		"build:h5:prod": "cross-env BUILD_ENV=prod taro build --type h5",
		"build:rn": "taro build --type rn",
		"build:qq": "taro build --type qq",
		"build:quickapp": "taro build --type quickapp",
		"dev:weapp": "npm run build:weapp:dev -- --watch",
		"test:weapp": "npm run build:weapp:test -- --watch",
		"prod:weapp": "npm run build:weapp:prod -- --watch",
		"demo:weapp": "npm run build:weapp:demo -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5:dev -- --watch",
		"test:h5": "npm run build:h5:test -- --watch",
		"prod:h5": "npm run build:h5:prod -- --watch",
		"demo:h5": "npm run build:h5:demo -- --watch",
		"dev:rn": "npm run build:rn -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch",
		"lint": "eslint --fix --ext .ts,.tsx src",
		"prepare": "husky install"
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.16.5",
		"@i18n-chain/react": "^2.0.1",
		"@i18n-chain/taro": "^0.8.0",
		"@tarojs/cli": "^3.4.0",
		"@tarojs/components": "^3.4.0",
		"@tarojs/plugin-framework-react": "^3.4.0",
		"@tarojs/react": "^3.4.0",
		"@tarojs/router": "^3.4.0",
		"@tarojs/runtime": "^3.4.0",
		"@tarojs/taro": "^3.4.0",
		"axios": "^0.21.1",
		"echarts": "^5.4.0",
		"global": "^4.4.0",
		"moment": "^2.29.4",
		"nervjs": "^1.5.7",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.6",
		"redux": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"taro-react-echarts": "^1.1.3",
		"taro-ui": "^3.0.0-alpha.10"
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@tarojs/mini-runner": "^3.4.0",
		"@tarojs/webpack-runner": "^3.4.0",
		"@types/react": "^17.0.38",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/eslint-plugin": "^5.8.1",
		"@typescript-eslint/parser": "^5.8.1",
		"babel-preset-taro": "^3.3.19",
		"cross-env": "^7.0.3",
		"dayjs": "^1.9.5",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-config-taro": "^3.3.19",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.2.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"eslint-plugin-taro": "2.2.14",
		"eslint-plugin-typescript": "^0.14.0",
		"husky": "^7.0.2",
		"lint-staged": "^11.1.2",
		"moment": "^2.29.4",
		"nerv-devtools": "^1.5.7",
		"stylelint": "^14.2.0",
		"typescript": "^4.5.4"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"eslint --fix",
			"git add"
		]
	}
}
